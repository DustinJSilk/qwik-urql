{
  "name": "qwik-urql",
  "version": "0.3.0",
  "description": "Urql support for Qwik projects",
  "scripts": {
    "build": "qwik build",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "pnpm build.lib && np --no-cleanup",
    "start": "vite --open --mode ssr",
    "test.watch": "vitest src",
    "test.unit": "vitest run src",
    "test": "pnpm run test.unit",
    "qwik": "qwik"
  },
  "main": "./lib/index.qwik.cjs",
  "qwik": "./lib/index.qwik.mjs",
  "module": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=7"
  },
  "private": false,
  "type": "module",
  "packageManager": "pnpm@7.16.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DustinJSilk/qwik-urql.git"
  },
  "keywords": [
    "Qwik",
    "City",
    "Urql",
    "GraphQL"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DustinJSilk/qwik-urql/issues"
  },
  "homepage": "https://github.com/DustinJSilk/qwik-urql#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.14.0",
    "@builder.io/qwik-city": "^0.0.123",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "8.4.10",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@urql/core": "^3.0.5",
    "@urql/exchange-auth": "^1.0.0",
    "@urql/exchange-execute": "^2.0.0",
    "@urql/exchange-graphcache": "^5.0.5",
    "cookie": "^0.5.0",
    "eslint": "8.28.0",
    "eslint-plugin-qwik": "^0.13.3",
    "graphql": "^16.6.0",
    "node-fetch": "3.3.0",
    "np": "7.6.2",
    "prettier": "2.7.1",
    "ts-essentials": "^9.3.0",
    "typescript": "4.9.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vite": "3.2.4",
    "vitest": "^0.25.0",
    "wonka": "^6.1.1"
  }
}
